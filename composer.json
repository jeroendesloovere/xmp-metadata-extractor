{
    "name": "jeroendesloovere/xmp-metadata-extractor",
    "description": "Extracting XMP metadata from images using PHP.",
    "type": "library",
    "keywords": ["php", "xmp", "metadata", "extractor", "image"],
    "homepage": "https://github.com/jeroendesloovere/xmp-data-extractor",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen Desloovere",
            "email": "info@jeroendesloovere.be"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": {
            "JeroenDesloovere\\XmpMetadataExtractor\\": "src/"
        },
        "exclude-from-classmap": [
            "**/tests/"
        ]
    }
}
